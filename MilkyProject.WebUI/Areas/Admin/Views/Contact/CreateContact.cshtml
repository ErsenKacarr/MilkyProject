@model MilkyProject.WebUI.Dtos.ContactDtos.CreateContactDto
@{
    ViewData["Title"] = "CreateContact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="content-wrapper">
    <br />
    <br />
    <br />
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Yeni İletişim Ekleme Paneli</h4>
                    <p class="card-description">
                        Aşağıdaki alanları eksiksiz doldurarak İletişim ekleme işlemini tamamlayabilirsiniz.
                    </p>
                    <form method="post">
                        <label>Adı</label>
                        <input type="text" asp-for="name" class="form-control" />
                        <br />
                        <label>E Mail</label>
                        <input type="text" asp-for="email" class="form-control" />
                        <br />
                        <label>Konu</label>
                        <input type="text" asp-for="subject" class="form-control" />
                        <br />
                        <label>Mesaj</label>
                        <input type="text" asp-for="message" class="form-control" />
                        <br />
                        <button class="btn btn-info">İletişim Kaydet</button>
                    </form>
                </div>
            </div>
        </div>

    </div>
</div>
